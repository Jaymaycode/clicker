import tkinter as tk

root = tk.Tk()
root.title("Click Me")

score = 0

def update_score():
    global score
    score += 1
    score_label.config(text="Score: " + str(score))

cookie_btn = tk.Button(root, text="üç™ Cookie", command=update_score)
cookie_btn.pack()

score_label = tk.Label(root, text="Score: 0")
score_label.pack()

root.mainloop()
